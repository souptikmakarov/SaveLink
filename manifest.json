{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "name": "SaveLinks",
  "description": "Save links to view later",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index_new.html"
  },
  "externally_connectable": {
    "matches": ["*://localhost/*"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}